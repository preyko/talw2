#include <iostream>
#include <fstream>

#include <stdio.h>
#include <string.h>

#include "RLType.h"
#include "RLCommand.h"
#include "RLInterpreter.h"

#include "RLPrecompiler.h"


#ifdef MAIN
int main(int argn,const char** arg) {
    if(argn == 2) {
        RLInterpreter::Initialization();
        RLPrecompiler::Precompiler(arg[1]);
        RLInterpreter::Perform();
    }

    return 0;
}
#endif

namespace RLPrecompiler {

std::ostream* logstream;

void setPrecompilerOutput(const char* lo) {
    logstream = new std::fstream("log.tmp",std::ios_base::in);
}

using namespace std;
